*!*	cDataLoc=GetDIR(cDataLoc,"Select Data Location")
*!*	lDataPathOk=.F.
*!*	IF LEN(TRIM(cDataLoc))>0
*!*		SET PATH TO &cDataLoc
*!*		if !file ("jkcusfil.dbf")
